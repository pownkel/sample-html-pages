{
  "branches": [
    {
      "name": "master",
      "prerelease": "develop"
    },
    "release"
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    [
      "@semantic-release/github",
      {
        "assets": [
          {
            "path": "README.md",
            "label": "readme"
          },
          {
            "path": "index.html",
            "label": "html"
          }
        ]
      }
    ]
  ]
}
